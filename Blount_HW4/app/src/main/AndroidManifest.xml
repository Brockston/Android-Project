<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.brocks.blount_hw4">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--activity android:name=".Login" /-->
        <activity android:name=".Signup" />
        <activity android:name=".MainActivity" />
        <!--activity android:name=".Medicine" /-->
        <activity android:name=".AddMed"
            android:parentActivityName=".Info"/>
        <activity android:name=".Info"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".CreateOrEditActivity"
            android:label="@string/title_activity_activity_edit"
            android:parentActivityName=".AddMed">
        </activity>
        <activity
            android:name="com.example.brocks.blount_hw4.Preferences"
            android:label="@string/app_name"
            android:parentActivityName=".Info">
        </activity>

    </application>

</manifest>